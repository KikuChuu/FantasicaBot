  cardMenuLeft() {
    x1 := this.CARD_MENU_SCROLL_X1
    y1 := this.CARD_MENU_SCROLL_Y1
    x2 := this.CARD_MENU_SCROLL_X2
    y2 := this.CARD_MENU_SCROLL_Y2

    MouseMove, this.CARD_MENU_SCROLL_X1, this.CARD_MENU_SCROLL_Y1
    tempSpeed := A_DefaultMouseSpeed
    SetDefaultMouseSpeed, 2
    SendEvent { click %x1%, %y1%, down }{ click %x2%, %y2%, up }
    SetDefaultMouseSpeed, tempSpeed
  }

  cardMenuRight() {
    x1 := this.CARD_MENU_SCROLL_X1
    y1 := this.CARD_MENU_SCROLL_Y1
    x2 := this.CARD_MENU_SCROLL_X2
    y2 := this.CARD_MENU_SCROLL_Y2

    MouseMove, this.CARD_MENU_SCROLL_X2, this.CARD_MENU_SCROLL_Y2
    tempSpeed := A_DefaultMouseSpeed
    SetDefaultMouseSpeed, 2
    SendEvent { click %x2%, %y2%, down }{ click %x1%, %y1%, up }
    SetDefaultMouseSpeed, tempSpeed
  }

