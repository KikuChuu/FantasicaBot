  __new() {
    global width, height
    this.detector := new Detector

; =================================================================================================
;   I had plans for these booleans abyssalDiceInStock & this.caveDiceInStock, but they aren't      
;   necessary variables to be using at this moment. Leave commented for futher development.
; -------------------------------------------------------------------------------------------------
;     this.abyssalDiceInStock := false 
;     this.caveDiceInStock := false
; =================================================================================================
    this.dirtDiceInStock := false

    this.cardsInStock := true
    this.allyCardsInStock := true
    this.cardMenuOpened := false
    this.diceMenuOpened := false
    this.inboxGifts := true

    if (width == 436 && height == 718) {
      ; =========================================
      ; The dice menu is transparent, so I had to 
      ; resort to hardcoding the menu's x & y 
      ; points (in pixels)
      ; -----------------------------------------
      this.DICE_MENU_X := "350"
      this.DICE_MENU_Y := "550"

      ; =========================================
      ; Same situation with the dice menu,
      ; transparency issues.
      ; -----------------------------------------
      this.CARD_MENU_X := "50"
      this.CARD_MENU_Y := "550"

      ; =========================================
      ; I wasn't sure how scrolling could be 
      ; implemented with any other method, so I 
      ; hardcoded the endpoints. One point is
      ; represented by X1 & Y1, the second point
      ; is represented by X2 & Y2.
      ; -----------------------------------------
      this.CARD_MENU_SCROLL_X1 := "60"
      this.CARD_MENU_SCROLL_Y1 := "500"
      this.CARD_MENU_SCROLL_X2 := "370"
      this.CARD_MENU_SCROLL_Y2 := "500" 
    }
    else {
      ; Leave empty for now. Will add additional hardcoded points here.
    }
  }

