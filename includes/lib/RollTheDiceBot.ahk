      if (this.detector.detect(this.BOARD_INBOX)) {
        clickAt(this.detector.foundPoint[1], this.detector.foundPoint[2])
        this.inboxGifts := false
        this.dirtDiceInStock := true ; Assumed we always get a dirt dice for simplicity, but if we don't, the bot will correctly handle itself.
      }
    }
