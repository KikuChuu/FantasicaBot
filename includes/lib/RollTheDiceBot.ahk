                this.closeDiceMenu()
              }
            }
            else if (this.isDirtDice()) {
              this.dirtDice()
              this.closeDiceMenu() 
            }
            else {
              this.dirtDiceInStock := false
              this.closeDiceMenu()
            }
          }
          else {
            this.closeCardMenu()
          }
        }
        else {
          clickAt(this.detector.foundPoint[1], this.detector.foundPoint[2]) ; Hotfix that clicks the candy icon to fix a bug of the game
        }
      }
    }
    else if (this.isBossBattle()) {
      while (this.isBossBattle()) {
        if (this.isBossRetryPrompt()) {
          this.acceptBossRetry()
        }
        else if (this.isEpicBossSharedPrompt()) { ; During the next roll the dice event, please visually check if this case happens during a boss battle
          this.epicBossSharedPrompt()
        }
        else if (this.isEpicBossLosePrompt()) {
          this.epicBossLosePrompt()
        }
        else if (this.isStopDice()) {
          this.stopDice()
        }
        else if (this.isRollDice()) {
          if (this.isAllyOneUsed()) {
            this.rollDice()
          }
          else if (this.isAllyTwoUsed()) {
            this.rollDice()
          }
          else if (this.isAllyThreeUsed()) {
            this.rollDice()
          }
          else if (this.allyCardsInStock) {
            this.cardMenu()
          }
          else {
            this.rollDice()
          }
        }
        else if (this.isCloseCardMenu()) {
          if (this.isUseCard()) {
            this.useCard()
          }
          else if (this.isAllyOneUsed()) {
            this.closeCardMenu()
          }
          else if (this.isAllyTwoUsed()) {
            this.closeCardMenu()
          }
          else if (this.isAllyThreeUsed()) {
            this.closeCardMenu()
          }
          else if (this.isAllyOne()) {
            this.allyOne()
          }
          else if (this.isAllyTwo()) {
            this.allyTwo()
          }
          else if (this.isAllyThree()) {
            this.allyThree()
          }
          else if (this.isMoreCardsRight()) {
            this.cardMenuRight()
          }
          else {
            this.allyCardsInStock := false
            this.closeCardMenu()
          }
        }
        else {
          clickAt(this.detector.foundPoint[1], this.detector.foundPoint[2]) ; Hotfix that clicks the candy icon to fix a bug of the game
        }
      }
    }
    else if (this.isResults()) {
      if (this.isBonusBoardWarpPrompt()) {
        this.acceptBonusBoardWarp()
      }
      else {
        this.board()
      }
    }
    else if (this.isEpicBossResults()) {
      this.epicBossResults()
    }
    else if (this.isMenu()) { ; During the next dice event, check whether this else-if can be executed while isBoard is true
      if (this.inboxGifts) {
        this.boardInbox()
      }
      else {
        this.closeBoardMenu()
      }
    }
  }
}
