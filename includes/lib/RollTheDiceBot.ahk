  cancelUseCardPrompt() {
    if (this.isUseCardPrompt()) {
      fromX := this.detector.foundPoint[1]
      fromY := this.detector.foundPoint[2]
      if (this.detector.detect(this.USE_CARD_PROMPT_CANCEL, fromX, fromY)) {
