  selectEpisode(theEpisode) {
    episode := this.getEpisodePath(theEpisode)
    loop {
      if (this.detector.detect(episode)) {
        fromX := this.detector.foundPoint[1]
        fromY := this.detector.foundPoint[2]
        if (this.detector.detect(this.SELECT, fromX, fromY, 150)) {
          clickAt(this.detector.foundPoint[1], this.detector.foundPoint[2])
          sleep 1000
          return true
        }
      }
      else if (this.isHiddenEpisodesTop()) {
        this.scrollTopEpisode()
      }
      else if (this.isLastPage()) {
        this.firstPage()
      }
      else {
        this.nextPage()
      }
    }
  }

  getEpisodePath(theEpisode) {
    if (theEpisode == 1) {
      return this.EPISODE_1
    }
    else if (theEpisode == 2) {
      return this.EPISODE_2
    }
    else if (theEpisode == 3) {
      return this.EPISODE_3
    }
    else if (theEpisode == 4) {
      return this.EPISODE_4
    }
    else if (theEpisode == 5) {
      return this.EPISODE_5
    }
    else if (theEpisode == 6) {
      return this.EPISODE_6
    }
    else if (theEpisode == 7) {
