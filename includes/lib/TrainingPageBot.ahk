  __new(theEpisode, theStage) {
    global width, height
    this.detector := Detector.getInstance()

    if (width == 436 && height == 718) {
      this.EPISODE_X1 := "200"
      this.EPISODE_Y1 := "400"
      this.EPISODE_X2 := "200"
      this.EPISODE_Y2 := "500"

      this.MENU_X1 := "200"
      this.MENU_Y1 := "285"
      this.MENU_X2 := "200"
      this.MENU_Y2 := "500"
    }
    else {
      this.EPISODE_X1 := "200"
      this.EPISODE_Y1 := "650"
      this.EPISODE_X2 := "200"
      this.EPISODE_Y2 := "800"

      this.MENU_X1 := "200"
      this.MENU_Y1 := "285"
      this.MENU_X2 := "200"
      this.MENU_Y2 := "620"
    }

    this.currentEpisode := theEpisode
    this.currentStage := theStage
  }

  isTrainingPage() {
    if (this.detector.detect(this.TITLE)) {
      return true
    }
    else {
      return false
    }
  }

  isHiddenEpisodesTop() {
    if (this.detector.detect(this.HIDDEN_EPISODES_TOP)) {
      return true
    }
    else {
      return false
    }
  }

  isHiddenEpisodesBottom() {
    if (this.detector.detect(this.HIDDEN_EPISODES_BOTTOM)) {
      return true
    }
    else {
      return false
    }
  }

  scrollDownEpisode() {
    episodeX1 := this.episode_X1
    episodeY1 := this.episode_Y1
    episodeX2 := this.episode_X2
    episodeY2 := this.episode_Y2 
    mouseMove, this.episode_X2, this.episode_Y2

    tempSpeed := A_DefaultMouseSpeed
    setDefaultMouseSpeed 30
    sendEvent {click, %episodeX2%, %episodeY2%, down}{click, %episodeX1%, %episodeY1%, up}
    setDefaultMouseSpeed, tempSpeed
    sleep 500
  }

  scrollBottomEpisode() {
    episodeX1 := this.episode_X1
    episodeY1 := this.episode_Y1
    episodeX2 := this.episode_X2
    episodeY2 := this.episode_Y2 
    mouseMove, this.episode_X2, this.episode_Y2

    tempSpeed := A_DefaultMouseSpeed
    setDefaultMouseSpeed 3
    loop, 2 {
      sendEvent {click, %episodeX2%, %episodeY2%, down}{click, %episodeX1%, %episodeY1%, up}
      sleep 500
    }
    setDefaultMouseSpeed, tempSpeed
  }

